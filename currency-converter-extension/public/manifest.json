{
  "manifest_version": 3,
  "name": "Currency Converter",
  "version": "1.0",
  "description": "A simple currency converter using React.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["activeTab", "storage"]
}
