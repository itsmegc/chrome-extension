{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "Get the current temperature of your city.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["geolocation"]
}
